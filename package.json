{
    "name": "datatables.net-scroller-bm",
    "description": "Scroller for DataTables with styling for [Bulma](https://bulma.io/)",
    "main": "js/scroller.bulma.js",
    "module": "js/scroller.bulma.mjs",
    "style": "css/scroller.bulma.css",
    "types": "./types/scroller.bulma.d.ts",
    "version": "2.2.0",
    "files": [
        "css/**/*.css",
        "js/**/*.js",
        "js/**/*.mjs",
        "types/**/*.d.ts"
    ],
    "keywords": [
        "Scroller",
        "Virtual Scrolling",
        "Bulma",
        "Datatables",
        "jQuery",
        "table",
        "filter",
        "sort"
    ],
    "dependencies": {
        "datatables.net-scroller": ">=2.1.1",
        "datatables.net-bm": ">=1.13.4",
        "jquery": ">=1.7"
    },
    "moduleType": [
        "globals",
        "amd",
        "node"
    ],
    "ignore": [
        "composer.json",
        "datatables.json",
        "package.json"
    ],
    "author": {
        "name": "SpryMedia Ltd",
        "url": "http://datatables.net"
    },
    "homepage": "https://datatables.net",
    "bugs": "https://datatables.net/forums",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/DataTables/Dist-DataTables-Scroller-Bulma.git"
    }
}